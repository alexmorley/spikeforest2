{
    "analysis_name": "synth_monotrode",
    "output":"key://pairio/spikeforest/spikeforest_analysis_results.synth_monotrode.json",
    "recordings":["key://pairio/spikeforest/spikeforest_recording_group.synth_monotrode.json"],
    "sorter_keys": ["ms4", "yass", "sc", "hs2", "tdc", "klusta", "waveclus"],
    "download_from": ["spikeforest.kbucket", "spikeforest.spikeforest2"],
    "upload_to": "spikeforest.kbucket",
    "job_timeout":1200,
    "sorters":{
        "waveclus": {
            "name": "Waveclus",
            "processor_name": "Waveclus",
            "params": {},
            "compute_resource":"cpu"
        },        
        "ms4": {
            "name": "MountainSort4",
            "processor_name": "MountainSort4",
            "params": {
                "detect_sign": -1,
                "adjacency_radius": 50,
                "detect_threshold": 3
            },
            "compute_resource":"cpu"
        },
        "irc": {
            "name":"IronClust",
            "processor_name":"IronClust",
            "params": {},
            "compute_resource": "gpu"
        },
        "irc-d2": {
            "name":"IronClust-drift2",
            "processor_name":"IronClust",
            "params": {
                "detect_sign": -1,
                "adjacency_radius": 50,
                "prm_template_name": "drift2_template.prm",
                "detect_threshold": 4.5
            },
            "compute_resource": "cpu"
        },
        "sc": {
            "name": "SpykingCircus",
            "processor_name": "SpykingCircus",
            "params": {
                "detect_sign": -1,
                "adjacency_radius": 50
            },
            "compute_resource": "cpu"
        },
        "ks": {
            "name": "KiloSort",
            "processor_name": "KiloSort",
            "params": {},
            "compute_resource": "gpu"
        },
        "ks2": {
            "name": "KiloSort2",
            "processor_name": "KiloSort2",
            "params": {},
            "compute_resource": "gpu"
        },
        "yass": {
            "name": "Yass",
            "processor_name": "Yass",
            "params": {},
            "compute_resource": "cpu"
        },
        "jrc": {
            "name": "JRClust",
            "processor_name": "JRClust",
            "params": {},
            "compute_resource": "gpu"
        },        
        "hs2": {
            "name": "HerdingSpikes2",
            "processor_name": "HerdingSpikes2",
            "params": {},
            "compute_resource": "cpu"
        },
        "klusta": {
            "name": "Klusta",
            "processor_name": "Klusta",
            "params": {},
            "compute_resource": "cpu"
        },        
        "tdc": {
            "name": "Tridesclous",
            "processor_name": "Tridesclous",
            "params": {},
            "compute_resource": "cpu"
        }  
    }
}
