{
    "analysis_name": "magland_synth",
    "output":"key://pairio/spikeforest/spikeforest_analysis_results.magland_synth.json",
    "recordings":["key://pairio/spikeforest/spikeforest_recording_group.magland_synth.json"],
    "sorter_keys": ["ms4", "yass", "ks", "ks2", "sc"],
    "sorter_keys_disable": ["ms4", "irc", "ks", "ks2", "yass", "sc"],
    "download_from": ["kbucket", "spikeforest.spikeforest2"],
    "job_timeout":1200,
    "compute_resources": {
        "default":{"resource_name":"ccmlin008-80", "collection":"spikeforest","share_id":"kbucket"},
        "cpu":{"resource_name":"ccmlin008-80", "collection":"spikeforest","share_id":"kbucket"},
        "gpu":{"resource_name":"ccmlin008-gpu", "collection":"spikeforest","share_id":"kbucket"}
    },
    "sorters":{
        "ms4": {
            "name": "MountainSort4-thr3",
            "processor_name": "MountainSort4",
            "params": {
                "detect_sign": -1,
                "adjacency_radius": 50,
                "detect_threshold": 3
            },
            "compute_resource":"cpu"
        },
        "irc": {
            "name":"IronClust-static",
            "processor_name":"IronClust",
            "params": {
                "detect_sign": -1,
                "adjacency_radius": 50,
                "prm_template_name": "static_template.prm",
                "detect_threshold": 4.5
            },
            "compute_resource": "cpu"
        },
        "irc-d2": {
            "name":"IronClust-drift2",
            "processor_name":"IronClust",
            "params": {
                "detect_sign": -1,
                "adjacency_radius": 50,
                "prm_template_name": "drift2_template.prm",
                "detect_threshold": 4.5
            },
            "compute_resource": "cpu"
        },
        "sc": {
            "name": "SpykingCircus",
            "processor_name": "SpykingCircus",
            "params": {
                "detect_sign": -1,
                "adjacency_radius": 50
            },
            "compute_resource": "cpu"
        },
        "ks": {
            "name": "KiloSort",
            "processor_name": "KiloSort",
            "params": {
                "detect_sign": -1,
                "adjacency_radius": 50
            },
            "compute_resource": "gpu"
        },
        "ks2": {
            "name": "KiloSort2",
            "processor_name": "KiloSort2",
            "params": {
                "detect_sign": -1,
                "adjacency_radius": 50
            },
            "compute_resource": "gpu"
        },
        "yass": {
            "name": "Yass",
            "processor_name": "Yass",
            "params": {
                "detect_sign": -1,
                "adjacency_radius": 50
            },
            "compute_resource": "cpu"
        }
    }
}
